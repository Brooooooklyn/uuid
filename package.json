{
  "name": "@napi-rs/uuid",
  "version": "0.2.0",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "uuid",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "arm-linux-androideabi",
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-freebsd"
      ]
    }
  },
  "files": [
    "index.js",
    "index.d.ts",
    "uuid.js",
    "uuid.d.ts"
  ],
  "license": "MIT",
  "devDependencies": {
    "@lukeed/uuid": "^2.0.0",
    "@napi-rs/cli": "^2.6.2",
    "@node-rs/deno-lint": "^1.13.0",
    "ava": "^4.2.0",
    "benny": "^3.7.1",
    "conventional-changelog-cli": "^2.2.2",
    "nanoid": "^3.3.2",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release --js uuid.js --dts uuid.d.ts",
    "build:debug": "napi build --platform  --js uuid.js --dts uuid.d.ts",
    "lint": "denolint .",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "version": "napi version && conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "packageManager": "yarn@3.2.0"
}
