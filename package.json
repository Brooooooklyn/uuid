  "files": [
    "index.js",
    "index.d.ts"
  ],
  "license": "MIT",
  "dependencies": {
    "@node-rs/helper": "^1.2.1"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.0.0-alpha.1",
    "@node-rs/deno-lint": "^1.9.2",
    "ava": "^3.15.0",
    "benny": "^3.6.15",
    "conventional-changelog-cli": "^2.1.1",
    "nanoid": "^3.1.28",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
